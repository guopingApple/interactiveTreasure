section{ background-size:auto 100%;background-repeat: no-repeat;background-position: center;}
.hint{
	height:  25px;
	width: 20px;
	background-size:226px;
	z-index: 99;
	position: absolute;
	left: 50%;
	margin-left:-10px; 			
	opacity: 1;
	bottom: 24px;
	-webkit-animation:hint-move 1.2s infinite ease;
}
@-webkit-keyframes hint-move{
	0%{opacity: 0 ;bottom: 3px;}
	70%{opacity: 1;bottom: 18px;}
	100%{opacity: 0;bottom: 24px;}
}	

.huxi{
	-webkit-animation-name: breathingLight;
	-webkit-animation-duration: 3s;
	-webkit-animation-iteration-count: infinite;
}	
@-webkit-keyframes breathingLight {
	from {
			-webkit-box-shadow:0 0 3px #fff;
	}
	50% {
			-webkit-box-shadow: 0 0 15px #fff;
		}
	to {
			-webkit-box-shadow:0 0 3px #fff;
		}
}	
p{animation-fill-mode:both}
.p01{position: absolute;left:-400px;top:18%; width: 80%;-webkit-transition: all 0.6s ease-in;-webkit-transition-delay:0s;}
.imgGold{position: absolute;right: 0;bottom: -7%;opacity: 0;}
.focus .imgGold{-webkit-animation: flipY 1s linear 1.6s infinite forwards;animation: flipY 1s linear 1.6s infinite forwards;}
@-webkit-keyframes flipY {
    0% {
        -webkit-transform: perspective(1200px) rotateZ(0);
        transform: perspective(1200px) rotateZ(0);
        opacity: 1;
    }

    100% {
        -webkit-transform: perspective(1200px) rotateZ(360deg);
        transform: perspective(1200px) rotateZ(360deg);
        opacity: 1
    }
}
@keyframes flipY {
    0% {
        -webkit-transform: perspective(1200px) rotateZ(0);
        transform: perspective(1200px) rotateZ(0);
        opacity: 1;
    }

    100% {
        -webkit-transform: perspective(1200px) rotateZ(360deg);
        transform: perspective(1200px) rotateZ(360deg);
        opacity: 1
    }
}
.focus .p01{left: 8%;}
.p02{position: absolute;left:10%;top:65%;width: 30%;    animation-delay: 1s;
