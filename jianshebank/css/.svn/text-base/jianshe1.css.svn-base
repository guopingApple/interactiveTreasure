#loading{
	width: 100%;
	height: 100%;
	position: fixed;
	top: 0;
	left: 0;
	background-color: #000;
	opacity: 0.6;
	display: -webkit-box;
	-webkit-box-align:center;
	-webkit-box-pack:center; z-index:10000;
}
#loading .icon{
	width: 32px;
	height: 32px;
}
#wrap{
	width: 100%;
	overflow: hidden;
	position: relative;
	height: 100%;
}
#scrollContent{
	width: 100%; height:100%;
	position: absolute;
	top: 0;
	left: 0;
	-webkit-transition:top 0.6s ease-out;
}
#scrollContent .content{
	width: 100%; height:100%;
	overflow: hidden;
	position: absolute;left: 0;top: 0; opacity:0;background: url(../images/back3.jpg) no-repeat center;background-size: cover;
} 
#scrollContent .focus{z-index: 100;opacity:1;-webkit-transition:opacity 1s ease 0s;}
