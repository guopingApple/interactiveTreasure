@charset "utf-8";
@font-face {
	font-family:"digital";
	src: url(font/DS-DIGIB.TTF);
}
body {
	margin: 0px;
	padding: 0px;
	font-size: 14px;
	overflow: hidden;
	font-family: "微软雅黑";
}

.page {
	position: absolute;
	overflow: hidden;
	display: none;
	left: 0;
	top: 0;
	background-color: #333333;
	background-size: cover;
	background-position: center;
}

.page_0 {
	display: block;
}

.page_0_type_area {
	width: 80%;
	height: auto;
	position: absolute;
	left: 10%;
	top: 40%;
	font-size: 16px;
	color: white;
	line-height: 30px;
}

.page_0_clickcontinue {
	text-align: center;
	margin-top: 10px;
	display: none;
}

.hide {
	display: none;
}

.page_mainimg {
	width: 100%;
	height: auto;
	position: absolute;
	left: 0;
	top: 0;
	background-color: #333333;
}

.page_1_switch {
	position: absolute;
	width: 8.3%;
	left: 15%;
	background-color: rgba(255, 0, 0, 0.4);
	box-shadow: #FF0000 0px 0px 5px;
	opacity: 0;
}

.page_1_switchact {
	animation: type_blink 0.5s 0.25s infinite alternate;
	-ms-animation: type_blink 0.5s 0.25s infinite alternate;
	-moz-animation: type_blink 0.5s 0.25s infinite alternate;
	-webkit-animation: type_blink 0.5s 0.25s infinite alternate;
}

.page_1_tip {
	position: absolute;
	width: 68%;
	height: auto;
	left: 16%;
	top: 62%;
	margin-top: 500px;
}

.page_2_tip {
	position: absolute;
	width: 68%;
	height: auto;
	left: 16%;
	top: 62%;
	margin-top: 500px;
}

.page_1_tipact {
	animation: page_1_tip 2s 0s ease forwards;
	-ms-animation: page_1_tip 2s 0s ease forwards;
	-moz-animation: page_1_tip 2s 0s ease forwards;
	-webkit-animation: page_1_tip 2s 0s ease forwards;
}

@keyframes page_1_tip {
	from {
		margin-top: 500px;
	}
	to {
		margin-top: 0px;
	}
}

@-ms-keyframes page_1_tip {
	from {
		margin-top: 500px;
	}
	to {
		margin-top: 0px;
	}
}

@-moz-keyframes page_1_tip {
	from {
		margin-top: 500px;
	}
	to {
		margin-top: 0px;
	}
}

@-webkit-keyframes page_1_tip {
	from {
		margin-top: 500px;
	}
	to {
		margin-top: 0px;
	}
}
