.type_blink {
	display: inline-block;
	width: 18px;
	height: 24px;
	line-height: 30px;
	text-align: center;
	vertical-align: top;
	border-bottom: solid 1px white;
	animation: type_blink 0.25s 0.25s infinite alternate;
	-ms-animation: type_blink 0.25s 0.25s infinite alternate;
	-moz-animation: type_blink 0.25s 0.25s infinite alternate;
	-webkit-animation: type_blink 0.25s 0.25s infinite alternate;
}

@keyframes type_blink {
	from {
		opacity: 1;
	}
	to {
		opacity: 0;
	}
}

@-ms-keyframes type_blink {
	from {
		opacity: 1;
	}
	to {
		opacity: 0;
	}
}

@-moz-keyframes type_blink {
	from {
		opacity: 1;
	}
	to {
		opacity: 0;
	}
}

@-webkit-keyframes type_blink {
	from {
		opacity: 1;
	}
	to {
		opacity: 0;
	}
}

.tip_box {
	width: 100%;
	height: 30px;
	clear: both;
	overflow: hidden;
	top: 0px;
	right: 0px;
	position: absolute;
	display: none;
}

.tip_box img {
	margin: 5px 0px 0px 5px;
	width: 25px;
	height: 25px;
	border-radius: 25px;
	background-color: rgba(255, 255, 255, 0.4);
	opacity: 0.3;
	float: left;
}

.loader {
	position: absolute;
	background-color: white;
	height: 8px;
	border-radius: 8px;
	width: 80%;
	top: 49%;
	left: 10%;
	overflow: hidden;
}

.loader_inner {
	background-color: #71DC68;
	width: 0px;
	height: 8px;
	float: left;
	border-radius: 8px;
}

.page_1_type_area {
	position: absolute;
	width: 80%;
	height: auto;
	top: 70%;
	left: 10%;
	color: white;
	font-size: 15px;
	line-height: 30px;
