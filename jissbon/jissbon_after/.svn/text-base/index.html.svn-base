		<script type="text/javascript">
			jQuery(function($) {
				var num;
				var filterClass = {
					blur: "blur",
					focus: "z_active"
				}
				var $w = $(window),
					$body = $("body")
				$loading = $("#loading"),
					$wrap = $("#wrap"),
					$scrollContent = $("#scrollContent"),
					$content = $("section");
				var contentLength = $content.size(),
					n = 1;

				function initPage() {
					var wH = $body.height();
					$wrap.css({
						"height": wH
					});
					$scrollContent.css({
						"height": '100%'
					});
					$content.css({
						"height": wH
					});
				}

				function resize() {
					initPage();
				}

				function windowLoad() {
					$("#loading").css({
						display: "none"
					});
					$content.first().removeClass('d_none').addClass('z_active');
					$('body').append('<audio id="music" loop="loop" autoplay="autoplay" src="img/yellow.mp3"></audio><div class="closemusic" style="width:30px;height:30px;background:url(img/music_on.png);background-size:cover;z-index: 1000;position: absolute;right: 10px;top:10px;border-radius: 50%;"></div>')
				}
				var onTouch = {
					startY: 0,
					moveY: 0,
					n_index: 0,
					currentTop: 0,
					onStart: function(e) {
						this.startY = e.pageY;
						this.currentTop = $scrollContent.css("top");
						//document.body.scrollTop=0;
						//event.preventDefault();
					},
					onMove: function(e) {
						//this.moveY=e.pageY;
						//console.log(this.moveY)
						event.preventDefault();
						//document.body.scrollTop=0;
					},
					onEnd: function(e) {
						//event.preventDefault();
						if (e.target.parentNode.nodeName.toLocaleLowerCase() == 'label') {
							this.fnAnswer();
						} else if ($(e.target).parent().prev().hasClass('clicked')) {
							this.goDown();
						} else if (this.startY < e.pageY - 50) {
							this.goUp();
						} else if (e.target.id == 'shareshow') {
							$('.page_result_share').removeClass('d_none');
							$('.page_result_share_img').removeClass('d_none');
						} else if (e.target.id == 'close') {
							$('.page_result_share').addClass('d_none');
							$('.page_result_share_img').addClass('d_none');
						} else if (e.target.id == 'retry') {
							location.reload();
							//							window.location.href = "";
						} else if ($(e.target).hasClass('closemusic')) {
							//							console.log('closemusic')
							var audio = document.getElementById('music');
							if (audio.paused) {
								audio.play();
								$('.closemusic').css({
									'background': 'url(img/music_on.png)',
									'background-size': 'cover'
								})
							} else {
								audio.pause();
								$('.closemusic').css({
									'background': 'url(img/music_off.png)',
									'background-size': 'cover'
								})
							};
						}
						//document.body.scrollTop=0;
					},
					goDown: function() {
						this.contentScroll("down");
					},
					goUp: function() {
						this.contentScroll("up");
					},
					contentScroll: function(flag) {
						if (flag == "down") {
							var index = this.filterClass("down");
							if (index == 7) {
								//这里是测试结果
								num = parseInt(Math.random() * 6);
								result = $('.result');
								result.find('img').eq(num).removeClass('d_none');
								$('.page_result').removeClass('d_none');
								var w = $(window).width(),
									btns_h,
									h = $(window).height(),
									result = $('.result img'),
									img_w = 0,
									btns = $('.page_result_btns');
								if (w == 320 && h == 480) {
									btns_h = btns.outerHeight();
									//									console.log(btns_h);
								} else {
									btns_h = btns.height();
									//									console.log(btns_h);
								}
								img_w = (w * h * 0.9) / h;
								result.width(img_w);
								$('.result').height(h - btns_h);
//								location.href = "index-result.html";
							}
						} else if (flag == "up" && top < 0) {
							//						$scrollContent.css({
							//							"top" : top + $body.height()
							//						});	
							//						this.filterClass("up");
							//						$('.page_result').addClass('d_none');
							event.preventDefault();
						};
						this.addClass();
					},
					getCurrentIndex: function() {
						var top = parseInt($scrollContent.css("top")),
							currentIndex = parseInt(top / $body.height());
						return Math.abs(currentIndex);
					},
					addClass: function(n) {
						var index = n || this.getCurrentIndex();
					},
					initAddClass: function() {
						this.addClass(0);
					},
					filterClass: function(flag) {
						this.n_index += 1;
						//						console.log(this.n_index)
						var index = this.n_index;
						if (flag == "up" && index < contentLength - 1) {
							$("#c" + index).addClass(filterClass.focus);
							$("#c" + (index + 1)).removeClass(filterClass.focus);
						} else if (flag == "down") {
							$("#c" + index).addClass(filterClass.focus).removeClass('d_none');
							$("#c" + (index - 1)).removeClass(filterClass.focus).addClass('d_none');
						};
						return index;
					},
					fnAnswer: function(e) {
						$(event.target).parent().addClass('checked').siblings().removeClass('checked');
						$(event.target).parents('.answerBox').addClass('clicked')
					}
				};
				initPage();
				window.onload = windowLoad;
				window.onresize = resize;
				//window.onscroll=function(){event.preventDefault()};
				window.onorientationchange = resize;
				document.body.addEventListener('touchstart', function(e) {
					e = e.changedTouches[0];
					onTouch.onStart(e);
				});
				document.body.addEventListener('touchmove', function(e) {
					onTouch.onMove(e.changedTouches[0], e);
				});
				document.body.addEventListener('touchend', function(e) {
					onTouch.onEnd(e.changedTouches[0]);
				});
			});
		</script>

